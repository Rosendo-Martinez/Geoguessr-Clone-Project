{
  "rules": {
     "leaderboard": {
      ".read": "auth !== null",
      "$uid": {
        "email": {
          ".write": "auth.uid === $uid",
          ".validate": "newData.isString()"
        },
        "score": {
          ".write": "auth.uid === $uid",
          ".validate": "newData.isNumber()"
        }
      }
     }
  }
}